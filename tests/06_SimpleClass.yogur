// The first example with classes, to check the foundations

class Container {
    var one : Int
    var two : Int
    def sum(one : Int, two : Int) -> result : Int {
        result = one - two
    }
    
}

def sum(one : Int, two : Int) -> result : Int {
    var useful : Container
    useful.one  = 3
    useful.two = 4
    result = one + two
}

